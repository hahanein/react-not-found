#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/@ladle+react@2.12.3_react-dom@18.2.0_react@18.2.0/node_modules/@ladle/react/lib/cli/cli.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/@ladle+react@2.12.3_react-dom@18.2.0_react@18.2.0/node_modules/@ladle/react/lib/cli/cli.js" "$@"
fi
